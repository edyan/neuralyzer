{
    "name": "edyan/neuralyzer",
    "description": "Library and CLI for Data anonymization",
    "type": "library",
    "keywords": ["anonymization"],
    "authors": [
        {
            "name": "Emmanuel Dyan",
            "email": "emmanueldyan@gmail.com"
        }
    ],
    "license": "GPL-2.0",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "fzaninotto/faker": "^1",
        "symfony/console": "^3",
        "symfony/yaml": "^3",
        "symfony/config": "^3",
        "symfony/stopwatch": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "phpunit/dbunit": "^3"
    },
    "bin": ["bin/anon"],
    "autoload": {
        "psr-4": {
            "Inet\\Neuralyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inet\\Neuralyzer\\Tests\\": "tests/"
        }
    }
}
