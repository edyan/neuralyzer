{
    "name": "edyan/neuralyzer",
    "description": "Library and CLI for Data anonymization",
    "type": "library",
    "keywords": ["anonymization", "database", "GDPR", "privacy"],
    "authors": [
        {
            "name": "Emmanuel Dyan",
            "email": "emmanueldyan@gmail.com"
        }
    ],
    "license": "GPL-2.0-or-later",
    "minimum-stability": "stable",
    "bin": ["bin/neuralyzer"],
    "require": {
        "php": ">=7.2.5",
        "ext-pdo": "*",
        "doctrine/dbal": "2.10.*",
        "fzaninotto/faker": "^1",
        "symfony/config": "^5",
        "symfony/console": "^5",
        "symfony/dependency-injection": "^5",
        "symfony/expression-language": "^5",
        "symfony/finder": "^5",
        "symfony/stopwatch": "^5",
        "symfony/yaml": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "consolidation/robo": "^2"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Edyan\\Neuralyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Edyan\\Neuralyzer\\Tests\\": "tests/"
        }
    }
}
